{
  "name": "nuxt3-awesome-starter",
  "version": "0.1.0",
  "description": "a Nuxt 3 starter template or boilerplate with a lot of useful features. Nuxt3 + Tailwindcss 3",
  "author": "viandwi24",
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port 3333",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "PORT=3334 pm2 start .output/server/index.mjs ",
    "serve": "serve dist/",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lintfix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "clean": "rm -rf .nuxt dist .output",
    "generate:locales": "node tools/translator.js ./locales en.yml"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@headlessui/vue": "^1.7.14",
    "@iconify/json": "^2.2.75",
    "@intlify/nuxt3": "^0.2.4",
    "@nuxt/content": "^2.6.0",
    "@nuxt/test-utils-edge": "3.6.0-28102385.525a06af",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@pinia/nuxt": "^0.4.11",
    "@unocss/nuxt": "^0.53.1",
    "@vueuse/core": "^10.1.2",
    "@vueuse/nuxt": "^10.1.2",
    "add": "^2.0.6",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.2.2",
    "nuxt": "3.5.3",
    "nuxt-windicss": "^2.6.1",
    "pinia": "^2.1.3",
    "pnpm": "^8.6.1",
    "postcss": "8.4.24",
    "postcss-loader": "^7.3.2",
    "prettier": "^2.8.8",
    "sass": "1.62.1",
    "sass-loader": "^13.3.1",
    "serve": "^14.2.0",
    "three": "^0.153.0",
    "translate": "^2.0.2",
    "typescript": "^5.1.3",
    "unplugin-icons": "^0.16.2",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vitest": "^0.32.0",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5",
    "webpack": "^5.85.1"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html}": [
      "pnpm lintfix"
    ]
  },
  "dependencies": {
    "@splidejs/vue-splide": "^0.6.12",
    "primeicons": "^6.0.1",
    "primevue": "^3.29.2"
  }
}